    {
        "id": "62f4f05f3db9bed4",
        "type": "function",
        "z": "08972a8227e69f46",
        "name": "Auto Set Lamp Color",
        "func": "if (global.get(\"lamp_mode\") == \"manual\"){\n    return msg;\n}\n\nfor (var L in [\"_L1\", \"_L2\"]) {\n    if (L == \"_L1\") {\n        var status = global.get(\"robot_status_R1\");\n    } else if ((L == \"_L2\")) {\n        var status = global.get(\"robot_status_R1\");\n    }\n\n    switch (status) {\n        case \"waiting\":\n            global.set(\"lamp_color\" + L, \"green\");\n            break;\n        case \"moving\":\n            global.set(\"lamp_color\" + L, \"blue\");\n            break;\n        case \"paused\":\n            global.set(\"lamp_color\" + L, \"yellow\");\n            break;\n        case \"locked\":\n            global.set(\"lamp_color\" + L, \"yellow\");\n            break;\n        case \"stopped\":\n            global.set(\"lamp_color\" + L, \"red\");\n            break;\n        default:\n            global.set(\"lamp_color\" + L, \"none\");\n            break;\n    }\n}\n\nreturn msg;",
