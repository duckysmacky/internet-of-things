    {
        "id": "c5b6b5e4cbc9febe",
        "type": "function",
        "z": "62c0e90bb8f0e164",
        "name": "Set Default",
        "func": "global.set(\"selected_robot\", \"1\");\nglobal.set(\"selected_limit\", \"loadProcMin\");\n\nfor (var R of [\"1\", \"2\"]) {\n    global.set(\"loadProcMin_R\" + R, {\n        \"m1\": 10,\n        \"m2\": 10,\n        \"m3\": 10,\n        \"m4\": 10,\n        \"m5\": 10,\n        \"m6\": 10\n    });\n    global.set(\"loadProcCritMin_R\" + R, {\n        \"m1\": 20,\n        \"m2\": 20,\n        \"m3\": 20,\n        \"m4\": 20,\n        \"m5\": 20,\n        \"m6\": 20\n    });\n    global.set(\"loadProcCritMax_R\" + R, {\n        \"m1\": 80,\n        \"m2\": 80,\n        \"m3\": 80,\n        \"m4\": 80,\n        \"m5\": 80,\n        \"m6\": 80\n    });\n    global.set(\"loadProcMax_R\" + R, {\n        \"m1\": 90,\n        \"m2\": 90,\n        \"m3\": 90,\n        \"m4\": 90,\n        \"m5\": 90,\n        \"m6\": 90\n    });\n    global.set(\"tempMin_R\" + R, {\n        \"m1\": 5,\n        \"m2\": 5,\n        \"m3\": 5,\n        \"m4\": 5,\n        \"m5\": 5,\n        \"m6\": 5\n    });\n    global.set(\"tempCritMin_R\" + R, {\n        \"m1\": 15,\n        \"m2\": 15,\n        \"m3\": 15,\n        \"m4\": 15,\n        \"m5\": 15,\n        \"m6\": 15\n    });\n    global.set(\"tempCritMax_R\" + R, {\n        \"m1\": 75,\n        \"m2\": 75,\n        \"m3\": 75,\n        \"m4\": 75,\n        \"m5\": 75,\n        \"m6\": 75\n    });\n    global.set(\"tempMax_R\" + R, {\n        \"m1\": 90,\n        \"m2\": 90,\n        \"m3\": 90,\n        \"m4\": 90,\n        \"m5\": 90,\n        \"m6\": 90\n    });\n}\n\nreturn msg;",
