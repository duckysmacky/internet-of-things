    {
        "id": "0f80fefce842fbf1",
        "type": "function",
        "z": "62c0e90bb8f0e164",
        "name": "Write Data",
        "func": "var thing = msg.payload;\n\nfor (var R of [\"1\", \"2\"]) {\n    if (thing.name == \"Robot1_\" + R) {\n        global.set(\"motors_R\" + R, {\n            \"m1\": thing.m1,\n            \"m2\": thing.m2,\n            \"m3\": thing.m3,\n            \"m4\": thing.m4,\n            \"m5\": thing.m5,\n            \"m6\": thing.m6\n        });\n        global.set(\"load_R\" + R, {\n            \"l1\": thing.l1,\n            \"l2\": thing.l2,\n            \"l3\": thing.l3,\n            \"l4\": thing.l4,\n            \"l5\": thing.l5,\n            \"l6\": thing.l6\n        });\n        global.set(\"temp_R\" + R, {\n            \"t1\": thing.t1,\n            \"t2\": thing.t2,\n            \"t3\": thing.t3,\n            \"t4\": thing.t4,\n            \"t5\": thing.t5,\n            \"t6\": thing.t6\n        });\n        global.set(\"s_R\" + R, thing.s);\n        global.set(\"n_R\" + R, thing.n);\n    }\n}\n\n// Terminal\nif (thing.name = \"WorkshiftTerminal1\") {\n    global.set(\"terminal\", {\n        \"p\": thing.p,\n        \"b1\": thing.b1,\n        \"b2\": thing.b2,\n        \"b3\": thing.b3\n    });\n}\n\nreturn msg;",
