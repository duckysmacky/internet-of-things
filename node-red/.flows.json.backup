    {
        "id": "1bca6d0e13d5ae14",
        "type": "function",
        "z": "08972a8227e69f46",
        "name": "Update Lamp Color",
        "func": "for (var L in [\"_L1\", \"_L2\"]) {\n    var color = global.get(\"lamp_color\" + L);\n    var output = global.get(\"output\" + L);\n\n    switch (color) {\n        case \"blue\":\n            output[\"L1\"] = \"1\";\n            output[\"L2\"] = \"0\";\n            output[\"L3\"] = \"0\";\n            output[\"L4\"] = \"0\";\n            break;\n        case \"red\":\n            output[\"L1\"] = \"0\";\n            output[\"L2\"] = \"1\";\n            output[\"L3\"] = \"0\";\n            output[\"L4\"] = \"0\";\n            break;\n        case \"yellow\":\n            output[\"L1\"] = \"0\";\n            output[\"L2\"] = \"0\";\n            output[\"L3\"] = \"1\";\n            output[\"L4\"] = \"0\";\n            break;\n        case \"green\":\n            output[\"L1\"] = \"0\";\n            output[\"L2\"] = \"0\";\n            output[\"L3\"] = \"0\";\n            output[\"L4\"] = \"1\";\n            break;\n        case \"none\":\n            output[\"L1\"] = \"0\";\n            output[\"L2\"] = \"0\";\n            output[\"L3\"] = \"0\";\n            output[\"L4\"] = \"0\";\n            break;\n    }\n\n    global.set(\"output\" + L, output);\n}\n\nreturn [\n    {payload: global.get(\"output_L1\")},\n    {payload: global.get(\"output_L2\")}\n];",
